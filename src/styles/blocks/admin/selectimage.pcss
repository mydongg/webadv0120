.selectimage{
    width: 100%;
    min-height: 300px;
    background-color: #dee4ed;
    position: relative;
    border: 1px dashed #a1a1a1;

    &__content{
        position: absolute;
        left: 50%;
        top: 50%;
        transform: translate(-50%,-50%);
        max-width: 50%;
        text-align: center;
    }

    &__text{
        margin-bottom: 30px;
        color: rgba(65, 76, 99, 0.502);
    }
}


