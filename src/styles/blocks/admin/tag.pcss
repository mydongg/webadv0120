.tag{
    display: inline-flex;
    background-color: #f4f4f4;
    border-radius: 16px;
    padding: 8px 16px;
    margin-right: 5px;
    margin-bottom: 5px;
    font-weight: 600;
    &__name{
        font-size: 16px;
        color: rgba(40, 51, 64, 0.702);
    }

    &__delete{
        margin-left: 8px;
    }

    &__svg{
        height: 12px;
        width: 12px;
        fill: rgba(40, 51, 64, 0.702);
    }
}

  